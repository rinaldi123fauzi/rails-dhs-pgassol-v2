<div class="page-header">
  <%= link_to detail_items_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_detail_item_path(@detail_item), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show detail_item</h1>
</div>

<dl class="dl-horizontal">
  <dt>Unit:</dt>
  <dd><%= @detail_item.unit %></dd>

  <dt>Dimension2:</dt>
  <dd><%= @detail_item.dimension2 %></dd>

  <dt>Dimension3:</dt>
  <dd><%= @detail_item.dimension3 %></dd>

  <dt>Dft:</dt>
  <dd><%= @detail_item.dft %></dd>

  <dt>Vol solid:</dt>
  <dd><%= @detail_item.vol_solid %></dd>

  <dt>Spreading rate:</dt>
  <dd><%= @detail_item.spreading_rate %></dd>

</dl>
