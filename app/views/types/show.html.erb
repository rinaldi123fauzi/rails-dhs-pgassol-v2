<div id="content">
  <div class="page-header">
    <%= link_to types_path, class: 'btn btn-default' do %>
      <span class="glyphicon glyphicon-list-alt"></span>
      Back
    <% end %>
    <%= link_to edit_type_path(@type), class: 'btn btn-primary' do %>
      <span class="glyphicon glyphicon-pencil"></span>
      Edit
    <% end %>
    <h1>Show type</h1>
  </div>

  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-default">
          <div class="panel-heading">
            Detail Type
          </div>
          <div class="panel-body">
            <table class="table table-bordered table-striped table-hover">
              <tbody>
              <tr>
                <td>Disipline:</td>
                <td><%= @getTypes.first.namedisiplines.try(:to_s) %></td>
              </tr>
              <tr>
                <td>Product:</td>
                <td><%= @getTypes.first.product_name.try(:to_s) %></td>
              </tr>
              <tr>
                <td>Name type:</td>
                <td><%= @getTypes.first.name_type.try(:to_s) %></td>
              </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
