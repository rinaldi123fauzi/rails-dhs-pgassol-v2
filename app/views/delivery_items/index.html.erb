<div class="page-header">
  <%= link_to new_delivery_item_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Delivery item
  <% end %>
  <h1>Listing delivery_items</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Delivery</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @delivery_items) do |delivery_item| %>
            <td><%= delivery_item.delivery %></td>
            <td><%= link_to 'Show', delivery_item %></td>
        <td><%= link_to 'Edit', edit_delivery_item_path(delivery_item) %></td>
        <td><%= link_to 'Destroy', delivery_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
